<parameters>
  <!-- general parameters -->
  <general>
    <verbose>1</verbose>
  </general>
  
  <!-- rnaSeq parameters -->
  <!-- averageRNASeq.m and readRNASeq.m -->
  <rnaSeq>
  	<data>2</data>
    <dir1>C:\FISHerMan\Db\mouse_frontal_cortex_mRNASeq_ENCFF653BKJ.xlsx</dir1>
	<dir2>C:\FISHerMan\Db\mouse_frontal_cortex_mRNASeq_ENCFF703SOK.xlsx</dir2>
	<mRNA>1</mRNA>
    <key1>ENS\w*T\d*</key1>
	<key2>ENS\w*G\d*</key2>
    <thres>0.1</thres>
  </rnaSeq>
  
  <!-- cdna parameters -->
  <!-- cdnaParse.m -->
  <cdna>
    <dir1>C:\FISHerMan\Db\Mouse38.cdna.fa</dir1>
    <key1>ENS\w*T\d*</key1>
	<key2>ENS\w*G\d*</key2>
  </cdna>
  
  <!-- ncrna parameters -->
  <!-- ncrnaParse.m -->
  <ncrna>
    <dir1>C:\FISHerMan\Db\Mouse38.ncrna.fa</dir1>
	<tRNA>1</tRNA>
	<dirT>C:\FISHerMan\Db\Mouse.trna.fas</dirT>
    <key1>ENS\w*T\d*</key1>
	<key2>ENS\w*G\d*</key2>
    <key3>gene_biotype:\S*</key3>
  </ncrna>
  
  <!-- abundantrna parameters -->
  <!-- abundantrnaParse.m -->
  <abundantrna>
    <percent>0.001</percent>
    <key1>ENS\w*T\d*</key1>
    <key2>:rRNA</key2>
	<key3>:Mt_rRNA</key3>
	<key4>:tRNA</key4>
	<key5>:Mt_tRNA</key5>
  </abundantrna>
  
  <!-- transcriptList parameters -->
  <!-- transcriptListParse.m -->
  <transcriptList>
    <dir1>C:\FISHerMan\Db\Mouse.transcriptList.fas</dir1>
    <length>40</length>
	<number>24</number>
  </transcriptList>
  
  <!-- OligoArray parameters -->
  <!-- runOligoArray.m -->
  <OligoArray>
    <GbMem>16</GbMem>
	<maxFragment>1000</maxFragment>
	<crosshybeT>72</crosshybeT>
	<crosshybeT>72.0</crosshybeT>
	<minTm>70</minTm>
	<maxTm>90</maxTm>
	<minPercentGC>40.0</minPercentGC>
	<maxPercentGC>65.0</maxPercentGC>
	<probeLength>30</probeLength>
	<probeLengthMax>30</probeLengthMax>
	<minProbeDist>40</minProbeDist>
	<maskedSeq>"GGGGGG;CCCCCC;TTTTTT;AAAAAA"</maskedSeq>
	<oligoArrayPath>C:\OligoArray\</oligoArrayPath>
	<oligoArrayExe>C:\OligoArray\OligoArray2.jar</oligoArrayExe>
    <blastDbPath>C:\OligoArray\BlastDb\</blastDbPath>
	<numParallel>10</numParallel>
  </OligoArray>
  
  <!-- oligos parameters -->
  <!-- oligosParse.m -->
  <oligos>
    <key1>ENS\w*T\d*</key1>
	<key2>ENS\w*G\d*</key2>
	<number>48</number>
	<seqNum>1000</seqNum>
	<thres>30</thres>
	<querySize>30</querySize>
	<DbSize>200000</DbSize>
    <blastArgs>-S 2</blastArgs>
	<parallel>0</parallel>
	<dir1>C:\FISHerMan\Db\Mouse.STList.fas</dir1>
  </oligos>
  
  <!-- adapters parameters -->
  <!-- appendAdapters.m -->
  <adapters>
    <dir1>C:\FISHerMan\Db\Mouse.alladapters.fas</dir1>
    <key1>ENS\w*T\d*</key1>
	<gf>GGAATCGTTGCGGGTGTCCT</gf>
	<grr>CCGCAACATCCAGCATCGTG</grr>
	<T7r>CCCTATAGTGAGTCGTATTA</T7r>
	<rRr>AGAGTGAGTAGTAGTGGAGT</rRr>
	<rGr>GATGATGTAGTAGTAAGGGT</rGr>
    <rBr>TGTGATGGAAGTTAGAGGGT</rBr>
	<rIRr>GGAGTAGTTGGTTGTTAGGA</rIRr>
  </adapters>
  
  <!-- 1stPCR parameters -->
  <!-- blast1stPCR.m -->
  <onePCR>
    <key1>ENS\w*T\d*</key1>
	<seqNum>1000</seqNum>
	<thres>22</thres>
	<querySize>20</querySize>
    <blastArgs>-S 3</blastArgs>
	<parallel>0</parallel>
	<gf>GGAATCGTTGCGGGTGTCCT</gf>
	<grr>CCGCAACATCCAGCATCGTG</grr>
  </onePCR>
  
</parameters>
