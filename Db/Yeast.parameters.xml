<parameters>
  <!-- general parameters -->
  <general>
    <verbose>1</verbose>
	<gkey1>cdna</gkey1>
	<gkey2>ncrna</gkey2>
	<gkey3>gene:\S*</gkey3>
	<gkey4>gene_biotype:\S*</gkey4>
	<number>48</number>
	<length>30</length>
	<gap>10</gap>
	<gf>GGAATCGTTGCGGGTGTCCT</gf>
	<grr>CCGCAACATCCAGCATCGTG</grr>
	<T7r>CCCTATAGTGAGTCGTATTA</T7r>
	<rRr>TGTGGAGGGATTGAAGGATA</rRr>
	<rGr>GGGAGAATGAGGTGTAATGT</rGr>
    <rBr>GATGATGTAGTAGTAAGGGT</rBr>
	<rIRr>AGGAGGAGGGTAATGATAGA</rIRr>
	<parallel>1</parallel>
	<seqNum>1000</seqNum>
  </general>
  
  <!-- rnaSeq parameters -->
  <!-- averageRNASeq.m and readRNASeq.m -->
  <rnaSeq>
  	<data>0</data>
    <dir1>C:\FISHerMan\Db\mouse_frontal_cortex_mRNASeq_ENCFF653BKJ.xlsx</dir1>
	<dir2>C:\FISHerMan\Db\mouse_frontal_cortex_mRNASeq_ENCFF703SOK.xlsx</dir2>
	<mRNA>1</mRNA>
    <key1>ENS\w*T\d*</key1>
	<key2>ENS\w*G\d*</key2>
    <thres>0.1</thres>
  </rnaSeq>
  
  <!-- cdna parameters -->
  <!-- cdnaParse.m -->
  <cdna>
    <dir1>C:\FISHerMan\Db\Yeast.cdna.fa</dir1>
  </cdna>
  
  <!-- ncrna parameters -->
  <!-- ncrnaParse.m -->
  <ncrna>
    <dir1>C:\FISHerMan\Db\Yeast.ncrna.fa</dir1>
	<tRNA>0</tRNA>
	<dirT>C:\FISHerMan\Db\Mouse.trna.txt</dirT>
  </ncrna>
  
  <!-- abundantrna parameters -->
  <!-- abundantrnaParse.m -->
  <abundantrna>
    <percent>0.001</percent>
    <key1>:rRNA</key1>
	<key2>:Mt_rRNA</key2>
	<key3>:tRNA</key3>
	<key4>:Mt_tRNA</key4>
  </abundantrna>
  
  <!-- transcriptList parameters -->
  <!-- transcriptListParse.m -->
  <transcriptList>
    <dir1>C:\FISHerMan\Db\Yeast.transcriptList.txt</dir1>
  </transcriptList>
  
  <!-- OligoArray parameters -->
  <!-- runOligoArray.m -->
  <OligoArray>
    <!-- 
	<GbMem>16</GbMem>
	<maxFragment>1000</maxFragment>
	<maskedSeq>"GGGGGG;CCCCCC;TTTTTT;AAAAAA"</maskedSeq>
	<oligoArrayPath>C:\OligoArray\</oligoArrayPath>
	<oligoArrayExe>C:\OligoArray\OligoArray2.jar</oligoArrayExe>
    <blastDbPath>C:\OligoArray\BlastDb\</blastDbPath>
	<numParallel>10</numParallel>
	<crosshybeT>75</crosshybeT>
	<secstructT>75.0</secstructT>
	<minTm>75</minTm>
	<maxTm>87</maxTm>
	<probeLength>30</probeLength>
	<probeLengthMax>30</probeLengthMax>
	<minProbeDist>40</minProbeDist>
	-->
	<minPercentGC>30.0</minPercentGC>
	<maxPercentGC>50.0</maxPercentGC>
  </OligoArray>
  
  <!-- oligos parameters -->
  <!-- oligosParse.m -->
  <oligos>
	<thres>30</thres>
    <blastArgs>-S 2</blastArgs>
	<dirST>C:\FISHerMan\Db\Yeast.STList.txt</dirST>
  </oligos>
  
  <!-- adapters parameters -->
  <!-- appendAdapters.m -->
  <adapters>
    <dir1>C:\FISHerMan\Db\Yeast.alladapters.txt</dir1>
  </adapters>
  
  <!-- 1stPCR parameters -->
  <!-- blast1stPCR.m -->
  <onePCR>
	<thres>22</thres>
	<querySize>20</querySize>
    <blastArgs>-S 3</blastArgs>
  </onePCR>
  
  <!-- otherSteps parameters -->
  <!-- blastOtherSteps.m -->
  <otherSteps>
	<thres>22</thres>
    <blastArgs1>-S 2</blastArgs1>
	<blastArgs2>-S 3</blastArgs2>
  </otherSteps>
  
  <!-- arna parameters -->
  <!-- blastAbundantRNA.m -->
  <arna>
	<thres>30</thres>
	<querySize>73</querySize>
    <blastArgs>-S 2</blastArgs>
  </arna>
  
  <!-- probeList parameters -->
  <!-- generateProbeList.m -->
  <probeList>
	<dirST>C:\FISHerMan\Db\Yeast.STList.txt</dirST>
  </probeList>
  
</parameters>
