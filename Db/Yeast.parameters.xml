<parameters>
  <!-- general parameters -->
  <general>
    <verbose>1</verbose>
	<key1>cdna</key1>
	<key2>ncrna</key2>
	<key3>gene:\S*</key3>
	<key4>gene_biotype:\S*</key4>
  </general>
  
  <!-- rnaSeq parameters -->
  <!-- averageRNASeq.m and readRNASeq.m -->
  <rnaSeq>
  	<data>0</data>
    <dir1>C:\FISHerMan\Db\mouse_frontal_cortex_mRNASeq_ENCFF653BKJ.xlsx</dir1>
	<dir2>C:\FISHerMan\Db\mouse_frontal_cortex_mRNASeq_ENCFF703SOK.xlsx</dir2>
	<mRNA>1</mRNA>
    <key1>ENS\w*T\d*</key1>
	<key2>ENS\w*G\d*</key2>
    <thres>0.1</thres>
  </rnaSeq>
  
  <!-- cdna parameters -->
  <!-- cdnaParse.m -->
  <cdna>
    <dir1>C:\FISHerMan\Db\Yeast.cdna.fa</dir1>
  </cdna>
  
  <!-- ncrna parameters -->
  <!-- ncrnaParse.m -->
  <ncrna>
    <dir1>C:\FISHerMan\Db\Yeast.ncrna.fa</dir1>
	<tRNA>0</tRNA>
	<dirT>C:\FISHerMan\Db\Mouse.trna.fas</dirT>
  </ncrna>
  
  <!-- abundantrna parameters -->
  <!-- abundantrnaParse.m -->
  <abundantrna>
    <percent>0.001</percent>
    <key1>:rRNA</key1>
	<key2>:Mt_rRNA</key2>
	<key3>:tRNA</key3>
	<key4>:Mt_tRNA</key4>
  </abundantrna>
  
  <!-- transcriptList parameters -->
  <!-- transcriptListParse.m -->
  <transcriptList>
    <dir1>C:\FISHerMan\Db\Yeast.transcriptList.fas</dir1>
    <length>40</length>
	<number>24</number>
  </transcriptList>
  
  <!-- OligoArray parameters -->
  <!-- runOligoArray.m -->
  <OligoArray>
    <GbMem>16</GbMem>
	<maxFragment>1000</maxFragment>
	<crosshybeT>72</crosshybeT>
	<crosshybeT>72.0</crosshybeT>
	<minTm>70</minTm>
	<maxTm>90</maxTm>
	<minPercentGC>40.0</minPercentGC>
	<maxPercentGC>65.0</maxPercentGC>
	<probeLength>30</probeLength>
	<probeLengthMax>30</probeLengthMax>
	<minProbeDist>40</minProbeDist>
	<maskedSeq>"GGGGGG;CCCCCC;TTTTTT;AAAAAA"</maskedSeq>
	<oligoArrayPath>C:\OligoArray\</oligoArrayPath>
	<oligoArrayExe>C:\OligoArray\OligoArray2.jar</oligoArrayExe>
    <blastDbPath>C:\OligoArray\BlastDb\</blastDbPath>
	<numParallel>10</numParallel>
  </OligoArray>
  
  <!-- oligos parameters -->
  <!-- oligosParse.m -->
  <oligos>
	<number>40</number>
	<seqNum>1000</seqNum>
	<thres>30</thres>
	<querySize>30</querySize>
	<DbSize>200000</DbSize>
    <blastArgs>-S 2</blastArgs>
	<parallel>0</parallel>
	<dirST>C:\FISHerMan\Db\Yeast.STList.fas</dirST>
  </oligos>
  
  <!-- reviewed until here -->
  <!-- adapters parameters -->
  <!-- appendAdapters.m -->
  <adapters>
    <dir1>C:\FISHerMan\Db\Mouse.alladapters.fas</dir1>
	<gf>GGAATCGTTGCGGGTGTCCT</gf>
	<grr>CCGCAACATCCAGCATCGTG</grr>
  </adapters>
  
  <!-- 1stPCR parameters -->
  <!-- blast1stPCR.m -->
  <onePCR>
	<seqNum>1000</seqNum>
	<thres>22</thres>
	<querySize>20</querySize>
    <blastArgs>-S 3</blastArgs>
	<parallel>0</parallel>
	<gf>GGAATCGTTGCGGGTGTCCT</gf>
	<grr>CCGCAACATCCAGCATCGTG</grr>
  </onePCR>
  
  <!-- otherSteps parameters -->
  <!-- blastOtherSteps.m -->
  <otherSteps>
	<thres>22</thres>
	<querySize>50</querySize>
    <blastArgs1>-S 2</blastArgs1>
	<blastArgs2>-S 3</blastArgs2>
	<grr>CCGCAACATCCAGCATCGTG</grr>
	<T7r>CCCTATAGTGAGTCGTATTA</T7r>
	<rRr>TGTGGAGGGATTGAAGGATA</rRr>
	<rGr>GGGAGAATGAGGTGTAATGT</rGr>
    <rBr>GATGATGTAGTAGTAAGGGT</rBr>
	<rIRr>AGGAGGAGGGTAATGATAGA</rIRr>
  </otherSteps>
  
  <!-- arna parameters -->
  <!-- blastAbundantRNA.m -->
  <arna>
	<seqNum>1000</seqNum>
	<thres>30</thres>
	<querySize>73</querySize>
	<DbSize>200000</DbSize>
    <blastArgs>-S 2</blastArgs>
	<parallel>0</parallel>
  </arna>
  
  <!-- probeList parameters -->
  <!-- generateProbeList.m -->
  <probeList>
	<number>40</number>
	<dirST>C:\FISHerMan\Db\Mouse.STList.fas</dirST>
  </probeList>
  
</parameters>
